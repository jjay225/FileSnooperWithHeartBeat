{
  "Serilog": {
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System.Net.Http.HttpClient.AzureHeartBeat.ClientHandler": "Verbose"
      }
    },
    "Properties": {
      "Application": "FileSnooper"
    },
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": { "path": "C:\\Logs\\Snooper\\LogFile.txt" }
      },
      { "Name": "Console" }

    ]
  },
  "FileMonitorPaths": [

    {
      "Source": "",
      "Target": ""
    },
    {
      "Source": "",
      "Target": ""
    }

  ],
  "FileTypeToMonitor": "*.sql",
  "SnoopDelayInMinutes": 1,
  "AzureHeartBeatServiceHostBase": "azurewebsites.com",
  "Identifier": "Snooper1",
  "AzureHeartBeatServiceFunctionUrlPath": "/api/Ping?code=34234"
}